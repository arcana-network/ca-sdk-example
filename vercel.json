{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "font-src 'self' https://*.cloudfront.net data:; img-src 'self' https://coin-images.coingecko.com data: ; script-src 'self' *.google-analytics.com *.googletagmanager.com; style-src 'self' 'unsafe-inline';"
                }
            ]
        }
    ]
}